{
  "name": "@prisma-cms/prisma-cms.com",
  "version": "4.3.0",
  "description": "Opensource framework CMS powered by GraphQL, Prisma, MySQL, React & Apollo GraphQL",
  "private": true,
  "workspaces": [
    "packages/@prisma-cms/context",
    "packages/@prisma-cms/component",
    "packages/@prisma-cms/editor",
    "packages/@prisma-cms/query-builder",
    "packages/@prisma-cms/uploader",
    "packages/apollo-cms",
    "packages/@prisma-cms/front-editor"
  ],
  "scripts": {
    "dev": "nodemon",
    "generate:types": "NODE_PATH=./ ts-node -P tsconfig.server.json src/modules/gql/cli/generateTypes/run",
    "clean": "rimraf ./dist",
    "build": "next build && tsc --project tsconfig.server.json",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "cross-env NODE_ENV=production node dist/next/server/index.js",
    "types": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "jest",
    "test:api": "TEST_REAL_API=true jest",
    "test:all": "yarn lint && yarn types && yarn cy:run && yarn test",
    "build-less": "node-less-chokidar src/styles/less",
    "start-ssr": "NODE_ENV=production node --experimental-modules src/ssr",
    "start-ssr-dev": "NODE_ENV=production PRISMA_CMS_TIMELOG=true nodemon src/ssr",
    "start-server": "Sendmail=true yarn start",
    "start-server-dev": "Sendmail=true SendmailTest=true yarn dev",
    "deploy": "yarn generate:types",
    "build-api": "node --experimental-modules src/server/scripts/deploy/api-only",
    "analyze": "yarn source-map-explorer 'build/static/js/*.js'",
    "get-api-schema": "graphql get-schema -o src/schema/generated/api.graphql",
    "build-api-fragments": "node --experimental-modules src/server/scripts/deploy/build-api-fragments",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test-server": "NODE_OPTIONS=\"--experimental-modules\" yarn mocha --delay src/__tests__/server/index.js",
    "gen:types": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run types"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
